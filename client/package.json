{
  "name": "photo-album",
  "version": "1.0.0",
  "private": true,
  "description": "Serverless Photo album",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "deploy": "npm run build:h5 && sls deploy --debug",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "yugasun",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.1.5",
    "@tarojs/components": "3.1.5",
    "@tarojs/react": "3.1.5",
    "@tarojs/runtime": "3.1.5",
    "@tarojs/taro": "3.1.5",
    "axios": "^0.21.1",
    "cos-js-sdk-v5": "^1.2.8",
    "lodash": "4.17.15",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-use": "^17.2.3",
    "taro-ui": "^3.0.0-alpha.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.1.5",
    "@tarojs/webpack-runner": "3.1.5",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.1.5",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.1.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0"
  }
}
